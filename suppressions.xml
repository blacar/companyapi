<?xml version="1.0"?>
<!DOCTYPE suppressions PUBLIC
        "-//Puppy Crawl//DTD Suppressions 1.0//EN"
        "http://www.puppycrawl.com/dtds/suppressions_1_0.dtd">
<suppressions>
    <!-- EXCLUDE CHECKS -->
    <suppress checks="JavadocStyleCheck" files=".*"/>
    <suppress checks="JavadocVariable" files=".*"/>
    <suppress checks="JavadocMethod" files=".*"/>
    <suppress checks="JavadocType" files=".*"/>
    <!-- EXCLUDE FILES -->
    <suppress files="CompanyapiApplication\.java" checks="[a-zA-Z0-9]*"/>
</suppressions>